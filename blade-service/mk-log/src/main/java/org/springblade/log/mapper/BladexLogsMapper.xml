<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.log.mapper.BladexLogsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="bladexLogsResultMap" type="org.springblade.log.entity.BladexLogs">
        <result column="id" property="id"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="update_time" property="updateTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="create_dept" property="createDept"/>
        <result column="action" property="action"/>
        <result column="task_id" property="taskId"/>
        <result column="action_log" property="actionLog"/>
    </resultMap>


    <select id="selectBladexLogsPage" resultMap="bladexLogsResultMap">
        select * from bladex_logs where is_deleted = 0
    </select>

</mapper>
